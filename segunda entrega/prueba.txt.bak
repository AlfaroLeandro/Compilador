DIM < pivot, contador, promedio, actual, suma> AS < integer, integer, real, real, real >;
DISPLAY "Prueba.txt LyC Tema 2!";
DISPLAY "ingrese un val int: ";

pivot := a - 0b11110;

GET actual;
contador := 0b11110;
suma := 02.5+pivot;
while (contador <= 92) 
	contador := contador + 1;
	actual := (contador/0.342) + (contador* 0xFF);
	suma := suma + actual;
endwhile;

DISPLAY "La suma es: ";
DISPLAY suma;

if (actual > 2 && !(actual == 0))
	DISPLAY "actual es > que 2 y != de 0";
else
	if (actual < 2)
	DISPLAY “no es mayor que 2”;
	endif;
endif;

